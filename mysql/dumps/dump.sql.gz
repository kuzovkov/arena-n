(12, 'Расписание Ретро-Бара', '<p><span style="color: #ffffff;"><span style="color: #0000ff;">Расписание ретробара</span></span></p>\r\n<table style="height: 135px;" width="360">\r\n<tbody>\r\n<tr>\r\n<td><span style="color: #0000ff;">Вн-Пт</span></td>\r\n<td><span style="color: #0000ff;">12-23</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">Сб-Вс</span></td>\r\n<td><span style="color: #0000ff;">15-03</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">Понедельник</span></td>\r\n<td><span style="color: #0000ff;">Выходной</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n</tr>\r\n<tr>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n<td><span style="color: #0000ff;">&nbsp;</span></td>\r\n</tr>\r\n</tbody>\r\n</table>', 'schedule', 'Расписание Ретро-Бара', NULL, NULL, '2014-05-28 12:36:56', '2014-05-29 14:33:44', 'RETROBAR'),
